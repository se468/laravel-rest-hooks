    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.5",
        "illuminate/mail": "~5.5",
        "illuminate/filesystem": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.2|~1.0.0",
        "phpunit/phpunit" : "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "se468\\RestHooks\\": "src"
        }
