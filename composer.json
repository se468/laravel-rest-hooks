{
    "name": "se468/laravel-rest-hooks",
    "description": "Package that handles subscribing, deleting, and firing REST Hooks.",
    "keywords": ["laravel", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Seyong Cho",
            "email": "seyong@seyongcho.com"
        }
